{
	"name": "svelte",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host --port 8081",
		"build": "vite build",
		"preview": "vite preview --host --port 8081",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
